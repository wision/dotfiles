Config { font = "xft:Fixed-8"
	, bgColor = "black"
	, fgColor = "grey"
	, position = Top
	, lowerOnStart = True
	, commands = [
		  Run Network "enp0s31f6" ["-t","<rx> / <tx>","-H","200","-L","10","-h","#FFB6B0","-l","#CEFFAC","-n","#FFFFCC"] 10
		, Run Network "wlp61s0" ["-t","<rx> / <tx>","-H","200","-L","10","-h","#FFB6B0","-l","#CEFFAC","-n","#FFFFCC"] 10
		, Run Wireless "wlp61s0" ["-t","ssid: <essid>"] 10
		, Run Cpu ["-L","30","-H","60","-h","#FFB6B0","-l","#CEFFAC","-n","#FFFFCC","-w","3"] 10
		, Run Memory ["-t","Mem: <usedratio>%","-H","8192","-L","4096","-h","#FFB6B0","-l","#CEFFAC","-n","#FFFFCC"] 10
		, Run Swap ["-t","Swap: <usedratio>%","-H","1024","-L","512","-h","#FFB6B0","-l","#CEFFAC","-n","#FFFFCC"] 10
		, Run Com "uname" ["-s","-r"] "" 36000
		, Run Battery ["-t","AC: <acstatus> Battery: <left>%","-L","10","-H","80","--","-O","<fc=green>On</fc>","-o","<fc=red>Off</fc>","-l","red","-m","yellow","-h","green","-p","blue","-f","/sys/class/power_supply/ADP1/online"] 10
		, Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
		, Run StdinReader
		]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%StdinReader% }{ %cpu%   |   %memory% - %swap%   |   enp0s31f6: %enp0s31f6%   |   wlp61s0: %wlp61s0%   |   %wlp61s0wi%                                  %battery%   |   <fc=#ee9a00>%date%</fc>  |   %uname% |                                                  "
	}
